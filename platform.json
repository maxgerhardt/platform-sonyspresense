{
  "name": "sonyspresense",
  "title": "Sony SPresense",
  "description": "SPRESENSE is an IoT board computer that combines low power consumption with a GPS reception function and a high-resolution audio codec.",
  "homepage": "https://developer.sony.com/develop/spresense/",
  "license": "Apache-2.0",
  "keywords": [
    "dev-platform",
    "ARM",
    "Cortex-M",
    "Sony",
    "SPresense"
  ],
  "engines": {
    "platformio": "^5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maxgerhardt/platform-sonyspresense"
  },
  "version": "1.0.0",
  "frameworks": {
    "arduino": {
      "package": "framework-arduinosonyspresense",
      "script": "builder/frameworks/arduino.py",
      "description": "Arduino Wiring-based Framework allows writing cross-platform software to control devices attached to a wide range of Arduino boards to create all kinds of creative coding, interactive objects, spaces or physical experiences",
      "homepage": "https://github.com/maxgerhardt/spresense-arduino-compatible.git",
      "title": "Arduino"
    }
  },
  "packages": {
    "toolchain-gccarmnoneeabi": {
      "type": "toolchain",
      "owner": "platformio",
      "version": "~1.90201.0"
    },
    "framework-arduinosonyspresense": {
      "type": "framework",
      "optional": true,
      "owner": "maxgerhardt",
      "version": "https://github.com/maxgerhardt/spresense-arduino-compatible.git"
    },
    "tool-arduinosonyspresensesdk": {
      "type": "framework",
      "optional": true,
      "owner": "maxgerhardt",
      "version": "https://github.com/maxgerhardt/spresense-arduino-sdk-package.git"
    },
    "tool-spresense": {
      "type": "upload",
      "owner": "maxgerhardt",
      "version": "https://github.com/maxgerhardt/spresense-tools-package.git"
    },
    "tool-openocd": {
      "type": "uploader",
      "optional": true,
      "owner": "platformio",
      "version": "~2.1100.0"
    }
  }
}
